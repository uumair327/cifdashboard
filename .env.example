# Firebase Web App Configuration (guardiancare-prod)
VITE_FIREBASE_API_KEY=your_web_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=guardiancare-prod.firebaseapp.com
VITE_FIREBASE_DATABASE_URL=https://guardiancare-prod-default-rtdb.asia-southeast1.firebasedatabase.app
VITE_FIREBASE_PROJECT_ID=guardiancare-prod
VITE_FIREBASE_STORAGE_BUCKET=guardiancare-prod.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=480855529488
VITE_FIREBASE_APP_ID=your_web_app_id_here
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id_here